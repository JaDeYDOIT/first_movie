<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "https://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- paymentSeat.xml -->

<mapper namespace="paymentSeat">
	<insert id="insert"
		parameterType="kr.co.fmos.paymentSeat.PaymentSeatDTO">
		INSERT INTO payment_seat (payment_id, seat_x, seat_y) VALUES
		(#{payment_id}, #{seat_x}, #{seat_y})
	</insert>
</mapper>